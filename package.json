{
  "name": "mustream",
  "version": "0.0.0",
  "description": "Observable data with automatic micro-subscriptions.",
  "main": "lib/index.js",
  "scripts": {
    "test": "ava test",
    "prebuild": "rm -rf lib && mkdir -p lib",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "patch-release": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdeal/mustream.git"
  },
  "keywords": [
    "stream",
    "streaming",
    "subscriptions",
    "subscribe",
    "micro-subscriptions",
    "object",
    "data"
  ],
  "author": "Justin Deal <justin.deal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdeal/mustream/issues"
  },
  "homepage": "https://github.com/jdeal/mustream#readme",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.3",
    "sinon": "^1.17.3"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ],
      "retainLines": true
    }
  },
  "dependencies": {
    "invariant": "^2.0.0"
  }
}
